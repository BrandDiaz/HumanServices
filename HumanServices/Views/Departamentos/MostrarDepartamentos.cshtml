@model IEnumerable<HumanServices.Models.Departamentos>

@{
    ViewBag.Title = "MostrarDepartamentos";
}

<h2>Departamentos</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.CodigoDepart)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Nombre)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.CodigoDepart)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Nombre)
        </td>
        <td>
       
        </td>
    </tr>
}

</table>
